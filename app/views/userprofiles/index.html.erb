<h1> userprofiles Pages </h1>

<table>
  <thead>
    <tr>
      <th>Userid</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Name</th>
      <th>Date of birth</th>
      <th>Gender</th>
      <th>Phone number</th>
      <th>Email</th>
      <th>No of application</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @userprofiles.each do |userprofile| %>
      <tr>
        <td><%= userprofile.userid %></td>
        <td><%= userprofile.first_name %></td>
        <td><%= userprofile.last_name %></td>
        <td><%= userprofile.name %></td>
        <td><%= userprofile.date_of_birth %></td>
        <td><%= userprofile.gender %></td>
        <td><%= userprofile.phone_number %></td>
        <td><%= userprofile.email %></td>
        <td><%= userprofile.no_of_application %></td>
        <td><%= userprofile.status %></td>
        <td><%= link_to 'Show', userprofile %></td>
        <td><%= link_to 'Edit', edit_userprofile_path(userprofile) %></td>
        <td><%= link_to 'Destroy', userprofile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Userprofile', new_userprofile_path %>
