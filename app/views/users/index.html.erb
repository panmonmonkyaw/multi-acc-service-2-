<h1>Admin Lists</h1>

<table>
  <thead>
    <tr>
      <th>email</th>
      <th>password</th>
      <th>password_confirmation</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.password %></td>
        <td><%= user.password_confirmation %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Sign up', sing_up %>
